import java.util.Scanner;

public class AddArray {
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int n = sc.nextInt();
		int []A = new int[n];
		for( int i = 0;i<A.length;i++) {
			A[i]=sc.nextInt();
		}
		int []B = new int[n];
		for(int i =0;i<B.length;i++) {
			B[i]=sc.nextInt();
			
		}
		for(int i=0;i<n;i++) {
		int C= A[i] + B[i];
		System.out.print(C+" ");

		
		
		
	}
	}
}